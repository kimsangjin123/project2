<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybats-3-mapper.dtd">

<mapper namespace="kr.co.mock.dao.MockDao">
	<select id="st_list" resultType="kr.co.mock.dto.Stock1Dto">
		select * from stocks
	</select>
	
	<select id="st_content" resultType="kr.co.mock.dto.Stock1Dto">
		select * from stocks where code=#{param1}
	</select>
	
	<select id="get_point" resultType="Integer">
		select mileage from mock_invest order by userid=#{param1} desc limit 1;
	</select>
</mapper>